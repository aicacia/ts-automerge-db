		"eslint-plugin-svelte": "3",
		"globals": "16",
		"prettier": "3",
		"prettier-plugin-svelte": "3",
		"prettier-plugin-tailwindcss": "0",
		"svelte": "5",
		"svelte-check": "4",
		"tailwindcss": "4",
		"typescript": "5",
		"typescript-eslint": "8",
		"vite": "7",
		"vite-plugin-devtools-json": "1",
		"vite-plugin-wasm": "3"
	},
	"dependencies": {
